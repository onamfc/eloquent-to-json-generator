{
    "name": "onamfc/eloquent-json-schema",
    "version": "dev-main",
    "description": "Generate JSON Schema files from Eloquent models for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "opis/json-schema": "^2.3"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "onamfc\\EloquentJsonSchema\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "onamfc\\EloquentJsonSchema\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "onamfc\\EloquentJsonSchema\\LaravelSchemaServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}